{% extends 'base.html.twig' %}

{% block title %}Performance{% endblock %}

{% block body %}
    <h1>Performance</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ performance.id }}</td>
            </tr>
            <tr>
                <th>Quantity</th>
                <td>{{ performance.quantity }}</td>
            </tr>
            <tr>
                <th>Designation</th>
                <td>{{ performance.designation }}</td>
            </tr>
            <tr>
                <th>Tax</th>
                <td>{{ performance.tax }}</td>
            </tr>
            <tr>
                <th>Pirce</th>
                <td>{{ performance.pirce }}</td>
            </tr>
            <tr>
                <th>Unit</th>
                <td>{{ performance.unit }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_performance_index') }}">back to list</a>

    <a href="{{ path('app_performance_edit', {'id': performance.id}) }}">edit</a>

    {{ include('performance/_delete_form.html.twig') }}
{% endblock %}
